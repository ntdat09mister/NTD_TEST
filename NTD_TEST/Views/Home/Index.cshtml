@{
    ViewData["Title"] = "Home Page";
}
@model List<Student> 
<div class="text-center">
    <h1 class="display-4">List Students</h1>
    <table>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Age</th>
        </tr>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Age</td>
            </tr>
        }
    </table>
</div>
